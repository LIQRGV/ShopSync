{
    "name": "thediamondbox/shopsync",
    "version": "2.0.1",
    "description": "A Laravel package for product management with flexible authentication and dual-mode operations (Database/API)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "products",
        "management",
        "crud",
        "api",
        "e-commerce",
        "inventory"
    ],
    "authors": [
        {
            "name": "LIQRGV",
            "email": "septianhari@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/validation": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.3",
        "laravel/pint": "^1.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TheDiamondBox\\ShopSync\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheDiamondBox\\ShopSync\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheDiamondBox\\ShopSync\\ProductPackageServiceProvider"
            ],
            "aliases": {
                "ProductService": "TheDiamondBox\\ShopSync\\Services\\ProductService"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}