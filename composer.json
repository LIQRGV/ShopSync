{
    "name": "liqrgv/shopsync",
    "verstion": "1.0.0",
    "description": "A Laravel package for product management with flexible authentication and dual-mode operations (Database/API)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "products",
        "management",
        "crud",
        "api",
        "e-commerce",
        "inventory"
    ],
    "authors": [
        {
            "name": "LIQRGV",
            "email": "septianhari@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.0|^11.0",
        "illuminate/validation": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.4",
        "laravel/pint": "^1.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Liqrgv\\ShopSync\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liqrgv\\ShopSync\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liqrgv\\ShopSync\\ProductPackageServiceProvider"
            ],
            "aliases": {
                "ProductService": "Liqrgv\\ShopSync\\Services\\ProductService"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}